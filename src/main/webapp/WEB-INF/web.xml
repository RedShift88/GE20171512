<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  
	<welcome-file-list>
		<welcome-file>plantList.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
        <servlet-name>createEquipment</servlet-name>
        <servlet-class>com.ge20171512.webapp.createEquipment</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>createEquipment</servlet-name>
        <url-pattern>/createEquipment</url-pattern>
    </servlet-mapping>
	
	<servlet>
        <servlet-name>createServiceTech</servlet-name>
        <servlet-class>com.ge20171512.webapp.createServiceTech</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>createServiceTech</servlet-name>
        <url-pattern>/createServiceTech</url-pattern>
    </servlet-mapping>
	
	<servlet>
        <servlet-name>createPlant</servlet-name>
        <servlet-class>com.ge20171512.webapp.createPlant</servlet-class>
    </servlet>
	
	<servlet-mapping>
        <servlet-name>createPlant</servlet-name>
        <url-pattern>/createPlant</url-pattern>
    </servlet-mapping>
</web-app>